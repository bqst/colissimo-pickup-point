{
  "name": "colissimo-pickup-point",
  "version": "0.0.1",
  "repository": "bqst/colissimo-pickup-point",
  "description": "Get the list of pickup points for a given address using the Colissimo API",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*",
    "index.js"
  ],
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "rimraf dist && tsc",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "colissimo",
    "laposte"
  ],
  "author": {
    "name": "Bastien MONTOIS",
    "email": "bastien.montois@gmail.com",
    "url": "https://www.bqst.fr"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.6.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.json"
        }
      ]
    },
    "testMatch": [
      "**/tests/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  }
}
